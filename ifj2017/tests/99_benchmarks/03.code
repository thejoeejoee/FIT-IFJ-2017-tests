' Tower of Hanoi for 4 rodes

function hanoi(n as integer, from as string, to_ as string, middle as string) as integer
	dim c as integer
	if n = 1 then
		print !"Move disk 1 from row "; from; !" to rod "; to_; !"\n";
		return 0
	end if
	c = hanoi(n - 1, from, middle, to_)
	print !"Move disk"; n; !" from row "; from; !" to rod "; to_; !"\n";
	c = hanoi(n - 1, middle, to_, from)
end function

scope
	dim c as integer
	c = hanoi(4, !"A", !"B", !"C")
end scope
